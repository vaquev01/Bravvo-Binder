    "vite": "^5.0.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
